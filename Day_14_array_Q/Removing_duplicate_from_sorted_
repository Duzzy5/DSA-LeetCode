def remove_duplicates(arr):
    if len(arr) == 0:
        return 0

    k = 1

    for i in range(1, len(arr)):
        if arr[i] != arr[k-1]:
            arr[k] = arr[i]
            k += 1

    return k
k=remove_duplicates(arr)
print("Total uniques values",k)
print("those unique values are",[:,k])
